{
    "name": "booj/stats-exporter",
    "description": "A package for Lumen/Laravel intended for use with booj projects to export statistics.",
    "type": "library",
    "authors": [
        {
            "name": "Samuel Collins",
            "email": "samuel.jackson.collins@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/lumen-framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "illuminated/testing-tools": "^6",
        "mockery/mockery": "~1.0",
        "phpunit/php-code-coverage": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Booj\\StatsExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Booj\\StatsExporter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Booj\\StatsExporter\\ServiceProvider"
            ]
        }
    }
}